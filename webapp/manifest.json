{
    "_version": "1.1.0",
    "sap.app": {
        "_version": "1.1.0",
        "id": "ui5.walkthrough",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": ["{{keyword1}}", "{{keyword2}}"]
        },
        "dataSources": {
            "invoiceRemote": {
                "uri": "https://services.odata.org/v2/Northwind/Northwind.svc/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "invoiceMock": {
                "uri": "/mockdata/",
                "type": "OData",
                "settings": {
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "_version": "1.1.0",
        "icons": {
            "icon": "icon.png",
            "favIcon": "favicon.ico",
            "phone": "phone-icon.png",
            "phone@2": "phone-icon@2x.png",
            "tablet": "tablet-icon.png",
            "tablet@2": "tablet-icon@2x.png"
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": ["sap_horizon", "sap_bluecrystal"]
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "resourceRoots": {
            "ui5.walkthrough": "./"
        },
        "dependencies": {
            "minUI5Version": "1.60",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ui.layout": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ui5.walkthrough.i18n.i18n",
                    "supportedLocales": [""],
                    "fallbackLocale": ""
                }
            },
            "invoice": {
                "dataSource": "invoiceMock"
            }
        },
        "rootView": {
            "viewName": "ui5.walkthrough.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "handleValidation": false,
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.platform.abap": {
        "_version": "1.1.0",
        "uri": "/sap/bc/ui5_ui5/sap/z_my_fiori_app/"
    },
    "sap.platform.hcp": {
        "_version": "1.1.0",
        "uri": "/hcp/path/to/your/app/"
    }
}
